{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    },
    "overrides": [
        {
            "files": [
                "__tests__/**"
            ],
            "rules": {
                "no-console": "error"
            }
        }
    ],
    "ignorePatterns": ["src/__tests__/**"],
    "root": true
}